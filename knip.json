{
  "$schema": "https://unpkg.com/knip@5/schema.json",
  "ignore": ["pnpm-workspace.yaml", "tempcontext/**"],
  "ignoreBinaries": [],
  "workspaces": {
    ".": {},
    "scripts": {
      "entry": [
        "check-turbo-inputs.ts",
        "check-dependency-health.ts",
        "lint-filenames.ts"
      ]
    },
    "packages/tasklens": {
      "project": ["src/**/*.{ts,tsx}!"]
    },
    "apps/client": {
      "entry": [
        "src/main.tsx",
        "tests/e2e/steps/**/*.steps.ts",
        "tests/e2e/fixtures.ts"
      ],
      "ignoreDependencies": ["@vitest/browser"]
    }
  }
}
