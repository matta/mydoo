{
  "$schema": "https://unpkg.com/knip@5/schema.json",
  "ignore": ["pnpm-workspace.yaml", "reference_repos/**", "crates/**"],
  "ignoreBinaries": [],
  "workspaces": {
    ".": {},
    "scripts": {
      "entry": ["check-dependency-health.ts", "sync-server.js"]
    },
    "packages/tasklens": {
      "project": ["src/**/*.{ts,tsx}!"]
    },
    "apps/client": {
      "entry": [
        "src/main.tsx",
        "tests/e2e/steps/**/*.steps.ts",
        "tests/e2e/fixtures.ts"
      ],
      "ignoreDependencies": ["@vitest/browser"]
    }
  }
}
