{
  "name": "@mydoo/tasklens",
  "version": "catalog:",
  "private": true,
  "description": "",
  "type": "module",
  "types": "./src/index.ts",
  "exports": {
    ".": "./src/index.ts"
  },
  "scripts": {
    "build": "tsc --emitDeclarationOnly",
    "check-types": "mkdir -p .tmp_gen && json2ts ./specs/compliance/schemas/test_case.schema.json .tmp_gen/test_case.d.ts && prettier --write .tmp_gen/test_case.d.ts && diff ./specs/compliance/schemas/test_case.d.ts .tmp_gen/test_case.d.ts",
    "fix": "eslint --fix \"{src,tests}/**/*.{ts,tsx}\"",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "generate-types": "json2ts ./specs/compliance/schemas/test_case.schema.json ./specs/compliance/schemas/test_case.d.ts && prettier --write ./specs/compliance/schemas/test_case.d.ts",
    "lint": "eslint \"{src,tests}/**/*.{ts,tsx}\"",
    "test": "vitest run --coverage",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@automerge/automerge-repo": "catalog:",
    "@automerge/automerge-repo-react-hooks": "catalog:",
    "react": "catalog:",
    "zod": "catalog:"
  },
  "devDependencies": {
    "@automerge/automerge": "catalog:",
    "@eslint/js": "catalog:",
    "@types/js-yaml": "catalog:",
    "@types/node": "catalog:",
    "@types/react": "catalog:",
    "@vitest/coverage-v8": "catalog:",
    "ajv": "catalog:",
    "ajv-formats": "catalog:",
    "eslint": "catalog:",
    "globals": "catalog:",
    "js-yaml": "catalog:",
    "json-schema-to-typescript": "catalog:",
    "prettier": "catalog:",
    "typescript": "catalog:",
    "typescript-eslint": "catalog:",
    "vitest": "catalog:"
  },
  "author": "",
  "keywords": [],
  "license": "ISC"
}
