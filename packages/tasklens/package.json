{
  "name": "@mydoo/tasklens",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "vitest run --coverage",
    "lint": "eslint \"{src,tests}/**/*.{ts,tsx}\"",
    "fix": "eslint --fix \"{src,tests}/**/*.{ts,tsx}\"",
    "typecheck": "tsc --noEmit",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "generate-types": "json2ts ./specs/compliance/schemas/test_case.schema.json ./specs/compliance/schemas/test_case.d.ts && prettier --write ./specs/compliance/schemas/test_case.d.ts",
    "check-types": "mkdir -p .tmp_gen && json2ts ./specs/compliance/schemas/test_case.schema.json .tmp_gen/test_case.d.ts && prettier --write .tmp_gen/test_case.d.ts && diff ./specs/compliance/schemas/test_case.d.ts .tmp_gen/test_case.d.ts"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@automerge/automerge": "^3.2.1",
    "@eslint/js": "^9.39.2",
    "@types/js-yaml": "^4.0.9",
    "@types/node": "^25.0.2",
    "@vitest/coverage-v8": "4.0.15",
    "ajv": "^8.17.1",
    "ajv-formats": "^3.0.1",
    "eslint": "^9.39.2",
    "globals": "^16.5.0",
    "js-yaml": "^4.1.1",
    "json-schema-to-typescript": "^15.0.4",
    "prettier": "^3.7.4",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.49.0",
    "vitest": "^4.0.15"
  }
}
