{
  "name": "mydoo",
  "description": "",
  "version": "0.0.0",
  "author": "",
  "devDependencies": {
    "@biomejs/biome": "catalog:",
    "@commitlint/cli": "catalog:",
    "@commitlint/types": "catalog:",
    "@ls-lint/ls-lint": "catalog:",
    "@types/js-yaml": "catalog:",
    "eslint": "catalog:",
    "eslint-plugin-import-x": "catalog:",
    "husky": "catalog:",
    "js-yaml": "catalog:",
    "knip": "catalog:",
    "lint-staged": "catalog:",
    "prettier": "catalog:",
    "syncpack": "catalog:",
    "tsx": "catalog:",
    "turbo": "catalog:",
    "typescript": "catalog:",
    "typescript-eslint": "catalog:"
  },
  "engines": {
    "node": ">=24"
  },
  "keywords": [],
  "license": "ISC",
  "packageManager": "pnpm@10.26.1",
  "private": true,
  "type": "module",
  "scripts": {
    "// Standard lifecycle scripts": "",
    "prepare": "husky",
    "//": "Turbo scripts",
    "build": "turbo run build",
    "check-agent": "turbo run check-agent",
    "check-biome": "turbo run check-biome --filter=!//",
    "check-eslint": "turbo run check-eslint --filter=!//",
    "check-human": "turbo run check-human",
    "check-style": "turbo run check-style --filter=!//",
    "check-types": "turbo run check-types --filter=!//",
    "check": "turbo run check --filter=!//",
    "dev": "turbo run dev",
    "fix": "turbo run fix --filter=!//",
    "generate": "turbo run generate",
    "test-e2e": "turbo run test-e2e",
    "test": "turbo run test",
    "verify": "turbo run fix --filter=!// && turbo run check-agent --filter=!//",
    "// Implementation scripts": "",
    "clean": "tsx scripts/smart-clean.ts",
    "check-biome-root": "biome check .",
    "check-deps-root": "knip",
    "check-filenames-root": "ls-lint",
    "check-format-root": "prettier --check \"*.{json,md,yaml,yml,js,ts,tsx,jsx,css,html}\"",
    "fix-biome-root": "biome check --write .",
    "fix-format-root": "prettier --write \"*.{json,md,yaml,yml,js,ts,tsx,jsx,css,html}\"",
    "lint-staged": "lint-staged",
    "check-staged": "./scripts/check-staged.sh",
    "check-turbo-root": "tsx scripts/check-turbo-inputs.ts",
    "check-eslint-root": "eslint . --max-warnings 0 --ignore-pattern 'packages/**' --ignore-pattern 'apps/**' --ignore-pattern 'scripts/**'",
    "check-syncpack-root": "syncpack list-mismatches",
    "check-catalog-root": "tsx scripts/check-unused-catalog-entries.ts",
    "fix-eslint-root": "eslint . --fix --ignore-pattern 'packages/**' --ignore-pattern 'apps/**' --ignore-pattern 'scripts/**'",
    "fix-syncpack-root": "syncpack fix-mismatches"
  }
}
