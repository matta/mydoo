    "lint": "turbo lint && pnpm lint:root",
    "lint:deps": "syncpack lint",
    "lint:root": "eslint --no-error-on-unmatched-pattern *.{js,mjs,ts}",
    "prepare": "husky",
    "test": "turbo test"
  },
  "devDependencies": {
    "@commitlint/cli": "catalog:",
    "@eslint/js": "catalog:",
    "eslint": "catalog:",
