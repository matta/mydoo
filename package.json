    "eslint-plugin-sonarjs": "catalog:",
    "eslint-plugin-unicorn": "catalog:",
    "globals": "catalog:",
    "husky": "catalog:",
    "lint-staged": "catalog:",
    "prettier": "catalog:",
    "syncpack": "catalog:",
    "turbo": "catalog:",
    "typescript": "catalog:",
    "typescript-eslint": "catalog:"
  },
  "author": "",
  "engines": {
    "node": ">=24"
  },
  "keywords": [],
