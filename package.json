{
  "name": "mydoo",
  "description": "",
  "version": "0.0.0",
  "author": "",
  "devDependencies": {
    "@biomejs/biome": "^2.3.10",
    "@commitlint/cli": "^20.2.0",
    "eslint": "catalog:",
    "eslint-plugin-import-x": "catalog:",
    "husky": "catalog:",
    "knip": "^5.78.0",
    "turbo": "^2.7.2",
    "prettier": "^3.4.2",
    "typescript": "catalog:",
    "typescript-eslint": "catalog:"
  },
  "engines": {
    "node": ">=24"
  },
  "keywords": [],
  "license": "ISC",
  "packageManager": "pnpm@10.26.1",
  "private": true,
  "type": "module",
  "scripts": {
    "verify": "turbo run fix --filter=!// && turbo run test test-e2e check --filter=!//",
    "build": "turbo build",
    "dev": "turbo dev",
    "check": "turbo run check --filter=!//",
    "fix": "turbo run fix --filter=!//",
    "fix-biome": "turbo run fix-biome --filter=!//",
    "fix-biome:root": "biome check --write .",
    "check-format": "prettier --check .",
    "fix-format": "prettier --write .",
    "format": "prettier --write .",
    "check-biome": "turbo check-biome --filter=!//",
    "check-biome:root": "biome check .",
    "check-eslint": "turbo check-eslint --filter=!//",
    "check-style": "turbo check-style --filter=!//",
    "check-deps": "knip",
    "check-biome-staged": "biome check --staged --no-errors-on-unmatched",
    "check-eslint-staged": "./scripts/check-eslint-staged.sh",
    "check-filenames-staged": "./scripts/check-filenames.sh --staged",
    "check-staged": "./scripts/check-staged.sh",
    "check-filenames": "./scripts/check-filenames.sh",
    "prepare": "husky",
    "check-context-staged": "./scripts/check-context-staged.sh",
    "check-agent": "turbo run check-agent",
    "check-human": "turbo run check-human",
    "check-format-staged": "./scripts/check-format-staged.sh",
    "test": "turbo test",
    "test-e2e": "turbo test-e2e",
    "check-types": "turbo check-types --filter=!//"
  }
}
